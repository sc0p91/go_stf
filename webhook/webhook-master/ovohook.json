[
  {
    "id": "webhook",
    "execute-command": "/opt/OV/bin/opcmsg",
    "command-working-directory": "/usr/sbin/webhook",
    "response-message": "message received",
    "response-headers":
    [
      {"name": "Access-Control-Allow-Origin","value": "*"},
	  {"name": "X-Example-Header", "value": "it works"}
    ],
    "pass-arguments-to-command":
    [
      {
        "source": "alarm",
        "name": "severity"
      },
      {
        "source": "alarm",
        "name": "application"
      },
      {
        "source": "alarm",
        "name": "object"
      }
	  {
        "source": "alarm",
        "name": "msg_grp"
      }
	  {
        "source": "alarm",
        "name": "msg_text"
      }
    ],
	"trigger-rule-mismatch-http-response-code": 500,
    "trigger-rule":
    {
      "and":
      [
        {
          "match":
			{
			"type": "ip-whitelist",
			"ip-range": "127.0.0.1/24"
			}
        },
        {
          "match":
          {
            "type": "name",
            "name": "msg_text",
          }
        }
      ]
    }
  }
]
